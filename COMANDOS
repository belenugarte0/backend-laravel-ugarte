//EJECUTAR POR PRIMERA VEZ
composer i
php artisan migrate
php artisan db:seed
php artisan serve

//REFRESH MIGRACION Y SEEDER
php artisan migrate:refresh
php artisan migrate:refresh --seed

//GENERAR CLAVE SECRETA JWT
php artisan jwt:secret

//ACTUALIZAR CAMBIOS GIT HUB
git pull origin main


//CREAR MIGRATION / CONTRALADOR / MODEL
php artisan make:model User -mrc

//CREAR CONTROLADOR FORMATO API
php artisan make:controller User/UserController --api
